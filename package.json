{
  "name": "ebrpl-system",
  "version": "2.0.0",
  "license": "MIT",
  "author": {
    "email": "annasmn34333@gmail.com",
    "name": "Muhammad Nur Annas"
  },
  "description": "Starter project (Angular + Electron + Nx + NestJs) for EBRPL System",
  "keywords": [
    "Angular",
    "Electron",
    "Nx",
    "NestJs",
    "Starter",
    "Project"
  ],
  "main": "dist/apps/electron/main.js",
  "scripts": {
    "postinstall": "npm run postinstall:electron && electron-builder install-app-deps",
    "postinstall-deps": "electron-builder install-app-deps",
    "postinstall:web": "node tools/builders/postinstall-web",
    "postinstall:electron": "node tools/builders/postinstall",
    "ng": "ng",
    "nx": "nx",
    "start": "npm run postinstall:electron && npm-run-all -p ng:serve electron:serve",
    "ng:serve": "ng serve browser",
    "ng:serve:web": "npm run postinstall:web && ng serve -o",
    "ng:serve:nest": "ng serve local-api",
    "ng:buil:nest": "ng build local-api",
    "electron:serve": "wait-on http-get://localhost:4200/ && npm run build:electron && electron dist/apps/electron/main.js --serve",
    "electron:local": "npm run build:prod && electron dist/apps/electron/main.js",
    "just:serve:electron": "electron dist/apps/electron/main.js",
    "electron:linux": "npm run build:prod && electron-builder build --linux",
    "electron:windows": "npm run build:prod && electron-builder build --windows",
    "just:build:electron:windows": "electron-builder build --windows",
    "electron:mac": "npm run build:prod && electron-builder build --mac",
    "build": "npm run postinstall:electron && npm run build:electron && ng build browser",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "build:electron": "ng build electron",
    "test:web": "ng test browser",
    "test:electron": "ng test electron",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.3",
    "@angular/cli": "~9.0.3",
    "@angular/compiler-cli": "~9.0.2",
    "@angular/language-service": "~9.0.2",
    "@compodoc/compodoc": "^1.1.10",
    "@nestjs/schematics": "^6.3.0",
    "@nestjs/testing": "^6.2.4",
    "@nrwl/angular": "9.0.3",
    "@nrwl/cypress": "9.0.3",
    "@nrwl/jest": "9.0.3",
    "@nrwl/nest": "9.0.3",
    "@nrwl/node": "9.0.3",
    "@nrwl/workspace": "9.0.3",
    "@types/jest": "^25.1.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "cypress": "^3.4.1",
    "dotenv": "6.2.0",
    "electron": "^7.2.4",
    "electron-builder": "^21.2.0",
    "electron-reload": "^1.5.0",
    "jest": "24.1.0",
    "jest-preset-angular": "^7.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "1.16.4",
    "spectron": "^8.0.0",
    "ts-jest": "24.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.6.4",
    "wait-on": "^3.3.0"
  },
  "dependencies": {
    "@angular/animations": "~9.0.2",
    "@angular/common": "~9.0.2",
    "@angular/compiler": "~9.0.2",
    "@angular/core": "~9.0.2",
    "@angular/forms": "~9.0.2",
    "@angular/platform-browser": "~9.0.2",
    "@angular/platform-browser-dynamic": "~9.0.2",
    "@angular/router": "~9.0.2",
    "@nestjs/common": "^6.11.6",
    "@nestjs/core": "^6.11.6",
    "@nestjs/ng-universal": "^2.0.1",
    "@nestjs/platform-express": "^6.2.4",
    "@nestjs/typeorm": "^6.2.0",
    "core-js": "^2.5.4",
    "reflect-metadata": "^0.1.12",
    "rxjs": "~6.5.4",
    "sqlite3": "^4.1.1",
    "tslib": "^1.10.0",
    "typeorm": "^0.2.22",
    "zone.js": "~0.10.2"
  }
}
